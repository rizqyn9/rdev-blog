{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "remix setup node",
    "clean": "rimraf ./node_modules/.cache ./server/dist ./build ./public/build \"./app/styles/**/*.css\"",
    "prebuild": "npm run clean && echo All clean âœ¨",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build": "yarn build:css && remix build",
    "dev": "pm2-dev ./other/pm2.config.js",
    "deploy": "fly deploy --remote-only",
    "start": "remix-serve build",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx ."
  },
  "dependencies": {
    "@kentcdodds/md-temp": "^1.1.0",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.1",
    "clsx": "^1.1.1",
    "dotenv-cli": "^5.0.0",
    "esbuild": "^0.14.21",
    "mdx-bundler": "^8.0.1",
    "node-gyp": "^8.4.1",
    "pm2": "^5.1.2",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reading-time": "^1.5.0",
    "rehype-document": "^6.0.1",
    "rehype-format": "^4.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-autolink-headings": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-slug": "^7.0.1",
    "remix": "^1.1.3",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.18",
    "unified": "^10.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "chokidar": "^3.5.3",
    "eslint": "^8.8.0",
    "eslint-config-kentcdodds": "^20.0.1",
    "husky": "^7.0.4",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.5",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "server-build",
    "build",
    "public/build",
    "*.ignored/",
    "*.ignored.*",
    "other/"
  ]
}
